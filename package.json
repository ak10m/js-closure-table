{
  "name": "closure-table",
  "description": "Closure Table on javascript",
  "version": "1.0.0",
  "license": "MIT",
  "author": "ak10m <akio.morimoto@airits.jp>",
  "homepage": "https://github.com/ak10m/js-closure-table#readme",
  "bugs": {
    "url": "https://github.com/ak10m/js-closure-table/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ak10m/js-closure-table.git"
  },
  "keywords": [
    "closure table",
    "tree",
    "hierarchy"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-typescript": "^7.6.0",
    "@types/jest": "^24.0.18",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.2"
  }
}
